= form_for(@venue) do |f|
  - if @venue.errors.any?
    #errorExplanation
      %h2= "#{pluralize(@venue.errors.count, 'error')} prohibited this venue from being saved:"
      %ul
      - @venue.errors.full_messages.each do |msg|
        %li= msg
  
  .field
    = f.label :name
    %br
    = f.text_field :name
  .field
    = f.label :address
    %br
    = f.text_field :address
  .field
    = f.label :city
    %br
    = f.text_field :city
  .field
    = f.label :url
    %br
    = f.text_field :url
  .field
    = f.label :description
    %br
    = f.text_area :description

  .actions
    = f.submit current_theme 'submit'
