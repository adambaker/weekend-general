= form_for(@venue) do |f|
  = render 'shared/error_messages', object: @venue
  
  .field
    = f.label :name
    %br
    = f.text_field :name
  .field
    = f.label :address
    %br
    = f.text_field :address
  .field
    = f.label :city
    %br
    = f.text_field :city
  .field
    = f.label :url
    %br
    = f.text_field :url
  .field
    = f.label :description
    %br
    = f.text_area :description

  .actions
    = f.submit current_theme 'venues', 'submit'
