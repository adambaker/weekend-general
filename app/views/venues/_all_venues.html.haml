%table
  %tbody
    - for venue in @venues
      %tr.entry_head{ class: cycle(:odd, :even) }
        %td.name= link_to venue.name, venue
        %td.address
          = venue.address
          %br/
          = venue.city
        %td.link= link_to(venue.url, venue.url) unless venue.url.blank?
      %tr
        %td
        %td.description(colspan=2)
          = search_display venue.description, @terms || []
